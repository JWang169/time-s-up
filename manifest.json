{
	"name": "counter",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Countdown extension - Leetcode special",
	"icons":{
		"16": "icon16.png",
		"148": "icon48.png",
		"128": "icon128.png"
	},
	"background":{
		"scripts": [
			"countdown.js"
		],
		"persistent": true
	},
	"browser_action":{
		"default_icon": "icon128.png"
	},
	"permissions": [
		"contextMenus"
	]
}